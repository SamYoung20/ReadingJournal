   "source": [
    "import random\n",
    "\n",
    "def birthday():\n",
    "    list1 =[]\n",
    "    for i in range(0, 23):\n",
    "        list1.append(random.randint(1,365))\n",
    "    return list1\n",
    "\n",
    "\n",
    "def main(j):\n",
    "    count = 0\n",
    "    for i in range(0,j):\n",
    "        if(has_duplicates(birthday())):\n",
    "            count = count + 1\n",
    "    return count/j\n",
    "\n",
    "\n",
    "main(100000)"
